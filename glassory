    # print to ini file
    inifile = open(iniFileName, "a")
    inifile.write("\n")


endOfFile = sheet3[sheet3.isnull().all(axis=1) == True].index.tolist()[0] # get final line number
ESNamesList = sheet3[Sheet3_column1].values.tolist()
